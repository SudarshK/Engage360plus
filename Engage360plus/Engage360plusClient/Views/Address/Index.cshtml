@model IEnumerable<Engage360plusUI.Models.DTO.AddressDto>
@{
}
<h1 class=" mt-3">Addresses</h1>

<table class="table table-bordered ">
    <thead>
        <tr>
            <th>Id</th>
            <th>City</th>
            <th>Country</th>
            <th>Region</th>
            <th>PostalCode</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var customer in Model)
        {
            <tr>
                <td>@customer.Id</td>
                <td>@customer.City</td>
                <td>@customer.Country</td>
                <td>@customer.Region</td>
                <td>@customer.PostalCode</td>
            </tr>
        }
    </tbody>
</table>